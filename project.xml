    </source>

		<source name="faw-protein" type="faw-protein" version="4.1.1">
			<property name="src.data.file" location="/storage/FAWMine/genome/cds/BGI/Sfr.pep.xml"/>
		</source>

		<source name="faw-protein2ipr" type="faw-protein2ipr" version="4.1.1">
